{
  "name": "Label Studio",
  "description": "Multi-type data labeling, annotation and exploration tool",
  "keywords": ["data annotation", "data labeling"],
  "website": "https://labelstud.io",
  "repository": "https://github.com/prathameshk/label-studio",
  "logo": "https://labelstud.io/images/opossum/heartex_icon_opossum_green.svg",
  "stack": "container",
  "env": {
    "LABEL_STUDIO_ONE_CLICK_DEPLOY": {
      "description": "Label Studio One Click Deploy Environmental Flag",
      "value": "1",
      "required": false
    },
    "DISABLE_SIGNUP_WITHOUT_LINK": {
      "description": "Disable signup for users without invite link",
      "value": "0",
      "required": false
    },
    "USERNAME": {
      "description": "Username(email) for default user",
      "value": "",
      "required": false
    },
    "PASSWORD": {
      "description": "Password for default user",
      "value": "",
      "required": false
    },
    "DJANGO_DB": {
      "description": "DJANGO_DB",
      "value": "default",
      "required": false
    },
    "POSTGRE_NAME": {
      "description": "POSTGRE_NAME",
      "value": "postgres",
      "required": false
    },
    "POSTGRE_USER": {
      "description": "POSTGRE_USER",
      "value": "postgres",
      "required": false
    },
    "POSTGRE_PASSWORD": {
      "description": "POSTGRE_PASSWORD",
      "value": "default",
      "required": false
    },
    "POSTGRE_PORT": {
      "description": "POSTGRE_PORT",
      "value": "5432",
      "required": false
    },
    "POSTGRE_HOST": {
      "description": "POSTGRE_HOST",
      "value": "",
      "required": false
    },
  }
}